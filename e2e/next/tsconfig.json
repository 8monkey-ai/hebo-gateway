{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "@hebo-ai/gateway": ["../../src/"],
      "@hebo-ai/gateway/*": ["../../src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules"]
}
